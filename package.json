  "module": "dist/esm/index.js",
  "scripts": {
    "format": "prettier -w src *.js *.json *.cjs",
    "build": "rollup -c rollup.config.cjs",
    "test": "mocha --timeout 5000",
    "lint": "eslint ./src/",
    "lint-fix": " eslint ./src/ --fix",
    "prepublishOnly": "gulp test",
